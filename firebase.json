{
  "hosting": {
    "headers": [ {
      "source": "*",
      "headers": [ {
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      } ]
    }],
    "site": "calendarblock",
    "public": "www",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "/www/**",
      "destination": "/index.html"
    },
    {
      "source": "**",
      "destination": "/index.html"
    }
  ]
  }
}
